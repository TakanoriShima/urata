<div class="container">
  <h2>
    <%= @to_user.user_profile.screen_name %>さんとの会話
  </h2>
  <ol id="messages">
    <% @messages.each do |message| %>
      <%= render 'layouts/messages/message', message: message %>
    <% end %>
  </ol>
  <%= render "layouts/messages/message_form", message: @message %>
</div>